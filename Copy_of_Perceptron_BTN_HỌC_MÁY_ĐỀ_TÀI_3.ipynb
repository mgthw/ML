        "<b><p style=\"font-size:16px\">1. KHAI BÁO THƯ VIỆN</b>"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "id": "d97ae94b",
      "metadata": {
        "id": "d97ae94b"
      },
      "outputs": [],
      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "class Perceptron:\n",
        "    def __init__(self, learning_rate=0.1, max_iter=1000):\n",
        "        self.learning_rate = learning_rate  # Tốc độ học, xác định mức độ điều chỉnh trọng số trong mỗi lần cập nhật\n",
        "        self.max_iter = max_iter # Số lần lặp tối đa để huấn luyện mô hình\n",
        "        self.weights = None  # Trọng số của mô hình, khởi tạo là none\n",
        "        self.bias = None  # Ngưỡng của mô hình, khởi tạo là none\n",
        "\n",
        "    def fit(self, X, y): # Huấn luyện mô hình, x là dữ liệu (input), y là nhãn (label)\n",
        "        n_samples, n_features = X.shape\n",
        "        self.weights = np.zeros(n_features)\n",
        "        self.bias = 0\n",
        "\n",
        "        for _ in range(self.max_iter):  # Lặp qua số lần tối đa\n",
        "            errors = 0\n",
        "            for i in range(n_samples):\n",
        "                linear_output = np.dot(X[i], self.weights) + self.bias\n",
        "                y_predicted = self.activation(linear_output)\n",
        "\n",
        "                update = self.learning_rate * (y[i] - y_predicted) # Cập nhật trọng số và bias dựa trên lỗi dự đoán\n",
        "                self.weights += update * X[i]\n",
        "                self.bias += update\n",
        "\n",
        "                errors += int(update != 0.0)  # Đếm lỗi, nếu giá trị cập nhật khác 0, mô hình dự đoán sai nên biến errors sẽ tăng lên 1\n",
        "            if errors == 0: # Không có lỗi thì dừng huấn luyện\n",
        "                break\n",
        "\n",
        "        self.coef_ = [self.weights]\n",
        "        self.intercept_ = self.bias\n",
        "\n",
        "    def predict(self, X): # Dự đoán mô hình\n",
        "        linear_output = np.dot(X, self.weights) + self.bias\n",
        "        # y_predicted = self.activation(linear_output)\n",
        "        y_predicted = [self.activation(output) for output in linear_output]\n",
        "        return y_predicted\n",
        "\n",
        "    def score(self, X, y):  # Đánh giá mô hình, so sánh dự đoán với nhãn thực tế\n",
        "        y_pred = self.predict(X)\n",
        "        return np.mean(y_pred == y)\n",
        "\n",
        "    def activation(self, x):  # Quyết định đầu ra của model dựa trên đầu vào tuyến tính\n",
        "        return 1 if x >= 0 else 0 # Nếu đầu vào >= 0, đầu ra là 1, ngược lại là 0"
      ],
      "metadata": {
        "id": "gYbzBxL8tpgU"
